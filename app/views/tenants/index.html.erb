<p id="notice"><%= notice %></p>

<h1>Tenants</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Lastname</th>
      <th>Dateofbirth</th>
      <th>Telephone</th>
      <th>Cellphone</th>
      <th>Gender</th>
      <th>Email</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tenants.each do |tenant| %>
      <tr>
        <td><%= tenant.name %></td>
        <td><%= tenant.lastName %></td>
        <td><%= tenant.dateOfBirth %></td>
        <td><%= tenant.telephone %></td>
        <td><%= tenant.cellphone %></td>
        <td><%= tenant.gender %></td>
        <td><%= tenant.email %></td>
        <td><%= tenant.password %></td>
        <td><%= link_to 'Show', tenant %></td>
        <td><%= link_to 'Edit', edit_tenant_path(tenant) %></td>
        <td><%= link_to 'Destroy', tenant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tenant', new_tenant_path %>
