<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Numberpersons</th>
      <th>Cost</th>
      <th>Body</th>
      <th>Type</th>
      <th>Idlessor</th>
      <th>Idtenant</th>
      <th>Idaddress</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.numberPersons %></td>
        <td><%= property.cost %></td>
        <td><%= property.body %></td>
        <td><%= property.type %></td>
        <td><%= property.idLessor %></td>
        <td><%= property.idTenant %></td>
        <td><%= property.idAddress %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
